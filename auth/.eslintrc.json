{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./auth/tsconfig.json"
    },
    "rules": {
      "@typescript-eslint/no-misused-promises":[
        "error",
        {
          "checksVoidReturn": false
        }
      ]
    }
}
